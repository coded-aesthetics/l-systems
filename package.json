        "start": "live-server --port=8080 --open=/index.html",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "gl-matrix": "^3.4.3"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/gl-matrix": "^2.4.5",
        "live-server": "^1.2.2",
        "typescript": "^5.0.0"
    },
    "keywords": [
        "l-systems",
        "fractals",
        "webgl",
        "3d",
        "typescript"
    ],
